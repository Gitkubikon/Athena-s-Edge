{
  "compilerOptions": {
    "strict": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ES2022",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "types": ["@types/node", "vite/client", "vitest/globals"],
    "resolveJsonModule": true,
    "paths": {
      "/*": ["./*"]
    }
  }
}
